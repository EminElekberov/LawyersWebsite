@model PageSlider

<table class="table table-bordered">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Descripton</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Id</td>
            <td>@Html.Raw(Model.Name)</td>
            <td>@Html.Raw(Model.Description)</td>
        </tr>
    </tbody>
</table>